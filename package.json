{
  "name": "nexgen-project",
  "version": "1.0.0",
  "description": "NexGen web application",
  "main": "backend/index.ts",
  "scripts": {
    "start": "ts-node ./backend/index.ts",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --watch backend --exec ts-node backend/index.ts",
    "dev:client": "live-server frontend",
    "build": "tsc"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongodb": "^4.1.0",
    "mongoose": "^5.13.5",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.7.1",
    "concurrently": "^6.2.0",
    "live-server": "^1.2.1",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  }
}
